{
	"name": "reflex-dom",
	"type": "module",
	"version": "0.14.13",
	"description": "Reflex is a tiny and fast reactive UI  library.",
	"main": "./reflex-dom/dist/index.es2017.cjs",
	"module": "./reflex-dom/dist/index.es2020.mjs",
	"unpkg": "./reflex-dom/dist/reflex.es2017.min.js",
	"types": "./reflex-dom/dist/index.d.ts",
	"exports": {
		".": "./reflex-dom/dist/index.es2020.mjs",
		"./hmr-plugin": "./reflex-dom/dist/hmr-plugin.es2020.mjs",
		"./hmr-runtime": "./reflex-dom/dist/hmr-runtime.es2020.mjs",
		"./debug": "./reflex-dom/dist/debug.es2020.mjs",
		"./render-to-string": "./reflex-dom/dist/renderToString.es2020.mjs"
	},
	"scripts": {
		"reset": "rm -rf node_modules && npm i",
		"clean": "rm -rf ./reflex-dom/dist/*",
		"build": "npm run clean && tsbundle build",
		"docs": "static-http ./pages/docs/",
		"publish": "npm run clean && tsbundle publish"
	},
	"author": "Alexis Bouhet (ZoulouX)",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.8.3",
		"@zouloux/static-http": "^1.0.0",
		"@zouloux/store": "^0.1.2",
		"@zouloux/tsbundle": "^1.4.0",
		"vite": "^4.1.3",
		"typescript": "^4.8.4"
	},
	"tsbundle": {
		"exportMap": {
			"Reflex": "./index"
		},
		"output": "./reflex-dom/dist",
		"formats": [
			"es2017.cjs",
			"es2020.mjs"
		],
		"files": [
			{
				"input": "./reflex-dom/src/index.ts",
				"formats": [
					"es2017.min.js"
				],
				"outName": "reflex",
				"exportBits": true
			},
			{
				"input": "./reflex-dom/src/index.ts"
			},
			{
				"input": "./reflex-dom/src/debug.ts"
			},
			{
				"formats": [
					"es2017.cjs",
					"es2020.mjs",
					"es2017.min.js"
				],
				"input": "./reflex-dom/src/renderToString.ts",
				"filterGlob": "renderToString.*"
			},
			{
				"input": "./reflex-dom/src/hmr-plugin.ts",
				"outName": "hmr-plugin",
				"filterGlob": "hmr-plugin.*"
			},
			{
				"input": "./reflex-dom/src/hmr-runtime.ts",
				"outName": "hmr-runtime",
				"filterGlob": "hmr-runtime.*"
			}
		]
	}
}
